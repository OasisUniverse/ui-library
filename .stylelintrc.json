{
    "plugins": [
        "stylelint-scss"
    ],
    "extends": [
        "stylelint-config-standard"
    ],
    "rules": {
        "indentation": 4,
        "max-line-length": 120,
        "no-missing-end-of-source-newline": null,
        "scss/dollar-variable-colon-space-after": "always",
        "at-rule-no-unknown": null
    }
}
